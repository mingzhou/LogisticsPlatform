<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.logistics"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Light.NoTitleBar" >
        
        <activity
            android:name="com.logistics.activity.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.logistics.activity.LoginActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.logistics.activity.MainActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.logistics.activity.GoodActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.logistics.activity.ProfileActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
